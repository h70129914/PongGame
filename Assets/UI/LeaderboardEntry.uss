.entry-container {
    flex-direction: row;
    width: 100%;
    min-height: 40px;
    margin-bottom: 10px;
    padding: 10px;
}

.index-label {
    align-self: flex-start;
    margin-right: 20px;
    font-size: 28px;
    color: rgb(255, 255, 255);
    -unity-text-align: middle-left;
}

.score-label {
    flex-grow: 1;
    align-self: center;
    font-size: 28px;
    color: rgb(255, 255, 255);
    -unity-text-align: middle-center;
}
